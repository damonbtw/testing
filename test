dx9.ShowConsole(false)

print("Script starting...")

--// Load the DXLib UI library
local success, Lib = pcall(function()
    return loadstring(dx9.Get("https://raw.githubusercontent.com/damonbtw/WHATISASKID/refs/heads/main/skiddingwhattt.lua"))()
end)

if not success then
    print("Failed to load library!")
    return
end

print("Library loaded!")

--// Create Window
local Window = Lib:CreateWindow({
    Title = "Chest ESP Test",
    Size = {400, 300},
    Resizable = false,
    ToggleKey = "[F5]"
})

print("Window created!")

Lib:Notify("ESP Loaded!", 3)
